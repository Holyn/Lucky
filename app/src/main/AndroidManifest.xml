<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dianxun.holyn.lucky">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".LuckyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.MainActivity2"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.MeActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.widget.webview.CustomWebViewActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.widget.picturesviewpager.PicturesViewPagerActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.activity.ClassifyActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.activity.ClassifyActivity2"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.activity.ClassifyContentListActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.activity.FoodDetailActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.activity.NewAnnounceActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.activity.OrderActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".view.activity.RoomActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.holyn.selectlocalimage.SelectLocalPicActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
